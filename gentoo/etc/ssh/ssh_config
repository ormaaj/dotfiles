Match originalhost anapnea.net,*.anapnea.net user saeed4u
	IdentityFile ~/.ssh/%r@%h.id_ed25519
	User saeed4u
	Port 443
	ControlPath none
	HashKnownHosts yes

Match localuser ormaaj,root
	IdentityFile ~/.ssh/ormaaj@%l.id_ed25519

Match originalhost anapnea.net,*.anapnea.net localuser ormaaj,root user ormaaj
	User ormaaj
	Port 443
	LocalForward 9091 localhost:9050
	DynamicForward localhost:9090
	HashKnownHosts yes

Match originalhost ndougl.as,*.ndougl.as
	User ormaaj
	DynamicForward localhost:9092
	HashKnownHosts yes

Match originalhost openwrt.ormaaj.org
	HostKeyAlgorithms +ssh-rsa-cert-v01@openssh.com,ssh-rsa
	Ciphers +aes256-ctr,aes192-ctr,aes128-ctr

Match originalhost *.github.com,github.com
	HostKeyAlgorithms +ssh-rsa-cert-v01@openssh.com,ssh-rsa
	User ormaaj
	ControlPath none
	LogLevel Debug

Host *
	StreamLocalBindUnlink yes
	UpdateHostKeys yes
	ProxyUseFdpass yes
	VerifyHostKeyDNS yes
	AddressFamily inet
	ForwardAgent no
	ForwardX11 no
	ForwardX11Timeout 52w
	ForwardX11Trusted no
	ServerAliveInterval 60
	ServerAliveCountMax 5
	PubKeyAuthentication yes
	RhostsRSAAuthentication no
	RSAAuthentication no
	PasswordAuthentication no
	HostbasedAuthentication no
	HashKnownHosts no
	SendEnv LANG LC_*
	ControlPath ~/.ssh/controlmasters/%C
	ControlMaster auto
	ControlPersist 0
	HostKeyAlgorithms ssh-ed25519-cert-v01@openssh.com,ssh-ed25519
	KexAlgorithms curve25519-sha256@libssh.org,diffie-hellman-group-exchange-sha256
	MACs hmac-sha2-512-etm@openssh.com,hmac-sha2-256-etm@openssh.com,umac-128-etm@openssh.com,hmac-sha2-512,hmac-sha2-256,umac-128@openssh.com
	Ciphers chacha20-poly1305@openssh.com,aes256-gcm@openssh.com,aes128-gcm@openssh.com
